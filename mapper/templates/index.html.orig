    <body>
        <div id="jqt">
            <div id="list" class="current">
                <div class="toolbar">
                    <a class="button leftButton" id="refreshList" href="#" onclick="selectDate();">Refresh</a>
                    <h1>List View</h1>
                    <a class="button" id="addEvent" href="#location_choice" >Add Event</a>
                </div>
                <div id="listview" class="scroll">
                    <ul>
                        <li>Loading..</li>
                    </ul>
                </div>
            </div>
            
            <div id="event">
                <div class="toolbar">
                    <a class="back" href="#" >Back</a>
                    <h1>Event View</h1>
                </div>
                <div class="scroll">
                    <ul>
                        <li>Loading..</li>
                    </ul>
                </div>
            </div>

            <div id="profile">
                <div class="toolbar">
                    <a href="#" class="back">Back</a>
                    <h1>My Interests</h1>
                </div>
            
                    <h2>My Events</h2>
                    <ul class="plastic">
                        <li class="slide arrow"><a href="#event">Burger Party</a></li>
                    </ul>
                    <h2>My tags</h2>
                    <ul class="plastic">
                        <li class="arrow"><a href="#old_interest">Hamburger</a></li>
                        <li class="arrow"><a href="#old_interest">CS</a></li>
                        <li class="arrow"><a href="#old_interest">Ice Cream</a></li>
                        <li class="forward"><a href="#add_interest">Add Interests</a></li>
                    </ul>
            </div>
        
            <div id="old_interest">
                <div class="toolbar">
                    <a href="#" class="back">Back</a>
                    <h1>Interest (Old) </h1>
                </div>
                        <a href="#profile" class="whiteButton">Remove from my interests</a>

            </div>  
            <div id="new_interest">
                <div class="toolbar">
                    <a href="#" class="back">Back</a>
                    <h1>Interest (New)</h1>
                </div>
                        <a href="#profile" class="whiteButton">Add to my interests</a>

            </div>
            <div id="add_interest">
                <div class="toolbar">
                    <a href="#" class="back">Back</a>
                    <h1>Browse Tags</h1>
                </div>
                
                <ul class="edgetoedge">
                    <li class="sep">C</li>
                    <li><a href="#new_interest">Cake</a></li>
                    <li><a href="#old_interest">CS</a></li>
                    <li class="sep">H</li>
                    <li><a href="#old_interest">Hamburger</a></li>
                    <li class="sep">I</li>
                    <li><a href="#old_interest">Ice Cream</a></li>
                    <li class="sep">S</li>
                    <li><a href="#new_interest">Supper</a></li>
                </ul>
            </div>
            
            
        
            <div id="location_choice">
                <div class="toolbar">
                    <a href="#" class="back">Back</a>
                </div>              
                <a href="#add_info" class="whiteButton" id="autofillButton">Here and now!</a>
                <a href="#date_location_chooser" class="whiteButton">Enter a time and location.</a>
            </div>
            
            <div id="date_location_chooser">
                <div class="toolbar">
                    <a href="#" class="back">Back</a>
                    <h1>Date and Time</h1>
                    <a href="#add_info" class="button">Continue</a>
                </div>
                <form>
                    <ul class="edit rounded">
                        <li><input id="time_chooser" type="text" value="11:45 PM" readonly onclick="selectDate();"/></li>
                        
                    </ul>
                    
                </form>
            </div>
                
            <div id="add_info">
                <div class="toolbar">
                    <h1>Add Event</h1>
                    <a href="#" class="back">Back</a>
                </div>
                <form>
                    <ul class="edit rounded">
                        <li><input type="text" name="name" placeholder="Event Name" id="name" /></li>
                        <li><input type="text" name="location" placeholder="Event Location" id="location"/></li>
                        <li><input type="text" name="time" placeholder="Time" id="time"  /></li>
                        <li>Repeating <span class="toggle"><input type="checkbox" name='repeating' /></span></li>
                        <li><textarea placeholder="Description" ></textarea></li>
                        <li><textarea placeholder="Add tags" ></textarea></li>
                        <li><a href="#home" class="whiteButton fade">Submit</a></li>
                    </ul>
                    
                </form>
            </div>
        
        </div>
    </body>
